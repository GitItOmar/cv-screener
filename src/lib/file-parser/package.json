{
  "name": "@screening/file-parser",
  "version": "1.0.0",
  "description": "Simplified file parser for CV screening - supports PDF and DOCX files",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./parsers/*": "./src/parsers/*.js",
    "./utils/*": "./src/utils/*.js"
  },
  "scripts": {
    "test": "node --test tests/**/*.test.js"
  },
  "dependencies": {
